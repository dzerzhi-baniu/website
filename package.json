{
  "name": "dzerzhi-baniu",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production --aot --base-href=/website --deploy-url=website/",
    "generate-assets": "DIRS_2_LIST=$(echo $(node util/list-dirs.mjs src/assets/images)) && node util/list-files.mjs src/assets/generated/images.json $DIRS_2_LIST",
    "process-assets": "bash -c \"sed -i -e 's/assets\\//website\\/assets\\//g' dist/dzerzhi-baniu/*.js; exit;\"",
    "push-2-gh-pages": "npx git-directory-deploy --directory dist/dzerzhi-baniu/ --branch gh-pages",
    "deploy": "npm run clean && npm run generate-assets && npm run build && npm run process-assets && npm run push-2-gh-pages",
    "clean": "rimraf dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@cds/core": "^6.3.2",
    "@clr/angular": "^15.1.0",
    "@clr/ui": "^15.1.0",
    "ng-gallery": "^8.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "git-directory-deploy": "^1.5.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rimraf": "^4.4.1",
    "typescript": "~4.9.4"
  }
}
